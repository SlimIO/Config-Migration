{
  "name": "@slimio/config-migration",
  "version": "0.1.0",
  "description": "SlimIO Configuration Migration",
  "main": "index.js",
  "scripts": {
    "prepublishOnly": "pkg-ok",
    "test": "cross-env psp && ava --verbose",
    "doc": "jsdoc -c ./jsdoc.json -r -R ./README.md -P ./package.json --verbose",
    "coverage": "nyc npm test",
    "report": "nyc report --reporter=html"
  },
  "engines": {
    "node": ">=10"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SlimIO/ConfigMigration.git"
  },
  "keywords": [
    "SlimIO",
    "config",
    "migration",
    "json"
  ],
  "author": "SlimIO",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SlimIO/ConfigMigration/issues"
  },
  "homepage": "https://github.com/SlimIO/ConfigMigration#readme",
  "nyc": {
    "check-coverage": true,
    "per-file": false,
    "lines": 98,
    "statements": 98,
    "functions": 98,
    "branches": 90
  },
  "husky": {
    "hooks": {
      "pre-push": "cross-env eslint index.js && npm test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^7.5.0",
    "@commitlint/config-conventional": "^7.5.0",
    "@slimio/eslint-config": "^2.0.4",
    "@slimio/psp": "^0.5.0",
    "ava": "^1.2.1",
    "cross-env": "^5.2.0",
    "eslint": "^5.13.0",
    "husky": "^2.0.0",
    "jsdoc": "^3.5.5",
    "nyc": "^14.0.0",
    "pkg-ok": "^2.3.1"
  },
  "dependencies": {
    "@slimio/is": "^1.4.0",
    "fast-json-patch": "^2.0.7",
    "lodash.clonedeep": "^4.5.0",
    "lodash.get": "^4.4.2",
    "lodash.set": "^4.3.2"
  }
}
